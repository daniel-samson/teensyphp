{
  "name": "daniel-samson/teensyphp",
  "description": "The minimalists web framework",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.2",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "files": [
      "src/router.php",
      "src/request.php",
      "src/response.php",
      "src/middleware.php",
      "src/template.php"
    ]
  },
  "require-dev": {
    "codeception/codeception": "2.6.x-dev"
  }
}
